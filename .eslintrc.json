{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "no-use-before-define": "error",
    "no-unused-vars": "error",
    "prettier/prettier": "error",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
